{
  "manifest_version": 2,
  "name": "Craigslist Visualizer",
  "version": "0.1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "browser_action" : {
    "default_title" : "CV"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "webview",
    "tabs",
    "storage",
    "http://vancouver.craigslist.ca/",
    "http://craigslist.ca/",
    "https://maps.googleapis.com/",
    "http://images.craigslist.org/"
  ],
  "content_scripts" : [{
    "js" : ["main.js"]
  }]
}