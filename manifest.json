{
  "manifest_version": 2,
  "name": "Craigslist Visualizer",
  "version": "0.1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "http://vancouver.craigslist.ca/"
  ]
}